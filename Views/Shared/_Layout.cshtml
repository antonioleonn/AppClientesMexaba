                        </li>
                    </ul>
                    <!--7-->
                    <ul>
                        <li class="menu-title"><span>Sistema</span></li>

                        <li>
                            <a href="login.html"><i class="fa fa-power-off"></i> <span>Cerrar Sesión</span></a>
                        </li>
                    </ul>

                </div>
            </div>
        </div>
        <!--TERMINA MENU-->

    </div>

    <div class="container">
        @RenderBody()
    </div>

    <div class="main-content">
        @RenderSection("content", required: false)
    </div>



    <!-- Scripts generales -->
    <script src="../../assets/js/jquery-3.7.0.min.js"></script>
    <script src="../../assets/js/bootstrap.bundle.min.js"></script>
